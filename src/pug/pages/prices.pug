    .container
        .progress-div
            .progress-line-div
                .progress-line
            .progress-onhold-div#mid
                .progress-onhold &#x2713;
            .progress-onhold-div#right
                .progress-onhold &#x2713;
            .progress-check-div
                .progress-check 3
                
        .header-div
            h2.header Find out how much your new iPhone costs
            p.subheader Select your device model to view prices.


    .container
