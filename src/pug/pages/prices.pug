extends ../template.pug

block header
    include ../regions/header.pug
    

block content
    
    .container
        .progress-div
            .progress-line-div
                .progress-line
            .progress-onhold-div#mid
                .progress-onhold &#x2713;
            .progress-onhold-div#right
                .progress-onhold &#x2713;
            .progress-check-div
                .progress-check 3
                
        .header-div
            h2.header Find out how much your new iPhone costs
            p.subheader Select your device model to view prices.


    .container
        .row.device-price-container
            .col-12.device-selection
                .col-xs-12.col-md-4.device-overview
                    .img-div
                        .img-wrapper(style="background-image:url('images/png/xs-2.png');")
                    .tagline#tagline-iphxs(style="background-image:url('images/png/logo-xs-1.png');")
                        span.visualHidden iPhone XS
                    .device-select#device-btn
                        .btn.active.slide-toggle(data-pricetab="iphxs-64") 64GB
                        .btn.slide-toggle(data-pricetab="iphxs-256") 256GB
                        .btn.slide-toggle(data-pricetab="iphxs-512") 512GB
                    //mobile-only btn
                    .dev-mobile-btn
                        .device-select-mob
                            .tagline(style="background-image:url('images/png/logo-xs-1.png');")
                                span.visualHidden iPhone XS
                            .btn.active.slide-toggle(data-pricetab="iphxs-64") 64GB
                            .btn.slide-toggle(data-pricetab="iphxs-256") 256GB
                            .btn.slide-toggle(data-pricetab="iphxs-512") 512GB
                        .device-select-mob
                            .tagline(style="background-image:url('images/png/logo-max-3.png');")
                                span.visualHidden iPhone XS Max
                            .btn.slide-toggle(data-pricetab="iphxs-max-64") 64GB
                            .btn.slide-toggle(data-pricetab="iphxs-max-256") 256GB
                            .btn.slide-toggle(data-pricetab="iphxs-max-512") 512GB
                
                .col-xs-12.col-md-4.device-overview.mt-xs-70
                    .img-div#hide-img
                        .img-wrapper(style="background-image:url('images/png/xs-max-2.png');")
                    .tagline#tagline-iphxs(style="background-image:url('images/png/logo-max-3.png');")
                        span.visualHidden iPhone XS Max
                    .device-select#device-btn
                        .btn.slide-toggle(data-pricetab="iphxs-max-64") 64GB
                        .btn.slide-toggle(data-pricetab="iphxs-max-256") 256GB
                        .btn.slide-toggle(data-pricetab="iphxs-max-512") 512GB
                        

                .col-xs-12.col-md-4.device-overview
                    .img-div
                        .img-wrapper(style="background-image:url('images/png/xr-2.png');")
                    .tagline(style="background-image:url('images/png/logo-xr-1.png');")
                        span.visualHidden iPhone XR
                    .device-select
                        .btn.slide-toggle(data-pricetab="iphxr-64") 64GB
                        .btn.slide-toggle(data-pricetab="iphxr-128") 128GB
