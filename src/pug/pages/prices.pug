            .col-12.device-selection
                .col-xs-12.col-md-4.device-overview
                    .img-div
                        .img-wrapper(style="background-image:url('images/png/xs-1.png');")
                    .tagline#tagline-iphxs(style="background-image:url('images/png/logo-xs.png');")
                        span.visualHidden iPhone XS
                    .device-select#device-btn
                        .btn.active.slide-toggle(data-pricetab="iphxs-64") 64GB
                        .btn.slide-toggle(data-pricetab="iphxs-256") 256GB
                        .btn.slide-toggle(data-pricetab="iphxs-512") 512GB
                
                .col-xs-12.col-md-4.device-overview.mt-xs-70
                    .img-div#hide-img
                        .img-wrapper(style="background-image:url('images/png/xs-max-1.png');")
                    .tagline#tagline-iphxs(style="background-image:url('images/png/logo-max.png');")
                        span.visualHidden iPhone XS Max
                    .device-select
                        .btn.slide-toggle(data-pricetab="iphxs-max-64") 64GB
                        .btn.slide-toggle(data-pricetab="iphxs-max-256") 256GB
                        .btn.slide-toggle(data-pricetab="iphxs-max-512") 512GB
                        

                .col-xs-12.col-md-4.device-overview
                    .img-div
                        .img-wrapper(style="background-image:url('images/png/xr-1.png');")
